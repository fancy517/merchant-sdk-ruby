= api_form_for @get_pal_details, :url => merchant_path(:get_pal_details) do |f|
  .row
    .span5
      %legend Get pal details request
      %p No parameters required. Just click `Submit`

  .form-actions
    = f.submit "Submit", :class => "btn btn-primary", :data => { :disable_with => "Processing.." }

- if @get_pal_details_response
  = render 'api_response', :request => @get_pal_details, :response => @get_pal_details_response
